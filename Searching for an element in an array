//search of an element in an array
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	int arr[] = new int []{18, 30, 15, 70, 12};
	int x=15;
	for(int i=0,k=0;i<arr.length;i++){
	    if(arr[i]==x){
	        System.out.println("Element found at "+ (i+1)+" index "+arr[i]);
	    }
	}
	
	}
}
